<MyWidget>:
    id: my_widget
    BoxLayout:
        orientation: "vertical"
        FileChooserListView:
            id: filechooser
            path: "."
            multiselect: True
            filters: ["*.obj"]
            on_selection: my_widget.selected(filechooser.selection)

        Label:
            pos_hint: {"x":0, "right":1}
            text: root.label_text
            font_size: 15
        Button:
            id: btn1
            font_size: 20
            background_color: 0.1, 0.5, 0.6, 1
            size_hint: 0.3, 0.3
            pos_hint: {"right":0.65}
            text: "Visualize"
            on_press: root.visualize(root.files)



